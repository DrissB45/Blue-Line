<form method="post" action="{{ path('app_admin_tutorial_delete', {'id': tutorial.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ tutorial.id) }}">
    <button type="button" class="p-2 mt-2 btn btn-outline-danger rounded-2">Supprimer</button>
</form>
